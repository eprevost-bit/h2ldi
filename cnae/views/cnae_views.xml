<odoo>

    <record id="view_cnae_list" model="ir.ui.view">
        <field name="name">cnae.cnae.list</field>
        <field name="model">cnae.cnae</field>
        <field name="arch" type="xml">
            <list>
                <field name="code"/>
                <field name="name"/>
                <field name="active"/>
            </list>
        </field>
    </record>
    <record id="view_cnae_form" model="ir.ui.view">
        <field name="name">cnae.cnae.form</field>
        <field name="model">cnae.cnae</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="name"/>
                        <field name="active"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_cnae" model="ir.actions.act_window">
        <field name="name">CNAE</field>
        <field name="res_model">cnae.cnae</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>
